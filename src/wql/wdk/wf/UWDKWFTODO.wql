	#对操作符进行判断，如果没有输入则提示错误
	IF 输入.flag = ""
		ERROR "：未输入操作符"
	ENDIF
	
	
	#新增
	IF 输入.flag = 1
		INSERT INTO wdk_wf_todo(
			id
			,pdid
			,piid
			,nodename
			,nodepage
			,nodetitle
			,nodetype
			,signtype
			,taskid
			,subtaskid
			,title
			,actorid
			,actortype
			,deptuuid
			,originactorid
			,originactortype
			,origindeptuuid
			,agenttype
			,opertype
			,opercomment
			,operdt
			,overduetime
			,lockactorid
			,lockactortype
			,lockdeptuuid
			,lockdt
			,sendactorid
			,sendactortype
			,senddeptuuid
			,senddt
			,status
			,todotype
			,ccstatus
			,turnid
			,unitid
			,pretodoid
			,limitdt
			,limittime
			,warnyellowdt
			,warnyellowtime
			,warnreddt
			,warnredtime
			,createdt
			,app_uuid
			,todosort
			,depute_id
    	) values (
			输入.id
			,输入.pdid
			,输入.piid
			,输入.nodename
			,输入.nodepage
			,输入.nodetitle
			,输入.nodetype
			,输入.signtype
			,输入.taskid
			,输入.subtaskid
			,输入.title
			,输入.actorid
			,输入.actortype
			,输入.deptuuid
			,输入.originactorid
			,输入.originactortype
			,输入.origindeptuuid
			,输入.agenttype
			,输入.opertype
			,输入.opercomment
			,输入.operdt
			,输入.overduetime
			,输入.lockactorid
			,输入.lockactortype
			,输入.lockdeptuuid
			,输入.lockdt
			,输入.sendactorid
			,输入.sendactortype
			,输入.senddeptuuid
			,输入.senddt
			,输入.status
			,输入.todotype
			,输入.ccstatus
			,输入.turnid
			,输入.unitid
			,输入.pretodoid
			,输入.limitdt
			,输入.limittime
			,输入.warnyellowdt
			,输入.warnyellowtime
			,输入.warnreddt
			,输入.warnredtime
			,输入.createdt
			,输入.app_uuid
			,输入.todosort
			,输入.depute_id
		)
		ENDINSERT
	ENDIF
	
	
	#修改意见
	IF 输入.flag = 21
		UPDATE wdk_wf_todo a SET
			a.operdt			=	输入.operdt
			,a.opercomment		=	输入.opercomment
		WHERE a.id = 输入.id
		ENDUPDATE
	ENDIF
	
	#修改意见及原始办理人
	IF 输入.flag = 22
		UPDATE wdk_wf_todo a SET
			a.operdt				=	输入.operdt
			,a.opercomment			=	输入.opercomment
			,a.actorid				=	输入.actorid
			,a.actortype			=	输入.actortype
			,a.deptuuid				=	输入.deptuuid
			,a.agenttype			=	输入.agenttype
			,a.originactorid		=	输入.originactorid
			,a.originactortype		=	输入.originactortype
			,a.origindeptuuid		=	输入.origindeptuuid
		WHERE a.id = 输入.id
		ENDUPDATE
	ENDIF
	
	#修改意见并结束待办
	IF 输入.flag = 23
		UPDATE wdk_wf_todo a SET
			a.operdt			=	输入.operdt
			,a.opertype			=	输入.opertype
			,a.opercomment		=	输入.opercomment
			,a.status		    = '1'
		WHERE a.id = 输入.id
		ENDUPDATE
		
		UPDATE wdk_wf_todo_his a SET
			a.operdt			=	输入.operdt
			,a.opertype			=	输入.opertype
			,a.opercomment		=	输入.opercomment
			,a.status		    = '1'
		WHERE a.id = 输入.id
		ENDUPDATE
	ENDIF
	
	#删除
	IF 输入.flag = 3
		DELETE FROM wdk_wf_todo a
		WHERE a.id = 输入.id
		ENDDELETE
	ENDIF
	
	
	
	#根据流程实例删除
	IF 输入.flag = 4
		DELETE FROM wdk_wf_todo a
		WHERE a.piid = 输入.piid
		ENDDELETE
	ENDIF
	#根据流程实例删除
	IF 输入.flag = 41
		DELETE FROM wdk_wf_todo a
		WHERE a.pdid = 输入.pdid
		ENDDELETE
	ENDIF
	
	
	
	#领取
	IF 输入.flag = 101
		#查询出当前任务所属的主任务
		SELECT
			taskid		as	taskid
			,subtaskid	as 	subtaskid
			,todotype	as	todotype
		FROM wdk_wf_todo a
		WHERE a.id = 输入.id
		ENDSELECT
		
		临时.taskid 		= taskid
		临时.subtaskid 	= subtaskid
		临时.todotype	= todotype
		
		#如果是正常待办，则以taskid和subtaskid为准
		IF 临时.todotype = 0
			#先将该主任务关联的所有其他待办关闭
			UPDATE wdk_wf_todo a SET
				a.status 		= '2'
			WHERE 1=1
				AND a.taskid 		= 临时.taskid
			OPTION 临时.subtaskid <> ""
				a.subtaskid		= 临时.subtaskid
			ENDOPTION
				AND a.id 		   <> 输入.id
				AND a.status        = '0'
			ENDUPDATE
			
			UPDATE wdk_wf_todo a SET
				a.lockactorid 		= 输入.lockactorid
				,a.lockactortype	= 输入.lockactortype
				,a.lockdeptuuid		= 输入.lockdeptuuid
				,a.lockdt			= 输入.lockdt
			WHERE 1=1
				AND a.id 			= 输入.id
			ENDUPDATE
		ELSE
		#如果是转办，则以ID为准
			UPDATE wdk_wf_todo a SET
				a.lockactorid 		= 输入.lockactorid
				,a.lockactortype	= 输入.lockactortype
				,a.lockdeptuuid		= 输入.lockdeptuuid
				,a.lockdt			= 输入.lockdt
			WHERE 1=1
				AND a.id 			= 输入.id
			ENDUPDATE
		ENDIF
		
	ENDIF
	
	#领退
	IF 输入.flag = 102
		#查询出当前任务所属的主任务
		SELECT
			taskid		as	taskid
			,subtaskid	as 	subtaskid
			,todotype	as	todotype
		FROM wdk_wf_todo a
		WHERE a.id = 输入.id
		ENDSELECT
		
		临时.taskid 		= taskid
		临时.subtaskid 	= subtaskid
		临时.todotype	= todotype
		
		#如果是正常待办，则以taskid和subtaskid为准
		IF 临时.todotype = 0
			#将该主任务关联的所有待办的当前locker均设置为领取人
			UPDATE wdk_wf_todo a SET
				a.lockactorid 		= ''
				,a.lockactortype	= ''
				,a.lockdeptuuid		= ''
				,a.lockdt			= ''
				,a.status 		= '0'
			WHERE 1=1
				AND a.taskid 		= 临时.taskid
			OPTION 临时.subtaskid <> ""
				a.subtaskid		= 临时.subtaskid
			ENDOPTION
				AND a.id 		   <> 输入.id
				AND a.status 		= '2'
			ENDUPDATE
			
			UPDATE wdk_wf_todo a SET
				a.lockactorid 		= ''
				,a.lockactortype	= ''
				,a.lockdeptuuid		= ''
				,a.lockdt			= ''
			WHERE 1=1
				AND a.id 			= 输入.id
			ENDUPDATE
		ELSE
		#如果是转办，则以ID为准
			UPDATE wdk_wf_todo a SET
				a.lockactorid 		= ''
				,a.lockactortype	= ''
				,a.lockdeptuuid		= ''
				,a.lockdt			= ''
			WHERE 1=1
				AND a.id 			= 输入.id
			ENDUPDATE
		
		ENDIF
	ENDIF
	
	
	#办理完毕
	IF 输入.flag = 103
		#1、查询当前待办所属的主任务
		SELECT
			taskid		as	taskid
			,subtaskid	as 	subtaskid
			,todotype	as	todotype
		FROM wdk_wf_todo a
		WHERE a.id = 输入.id
		ENDSELECT
		
		临时.taskid 		= taskid
		临时.subtaskid 	= subtaskid
		临时.todotype	= todotype
		
		
		#如果是正常待办，则以taskid和subtaskid为准
		IF 临时.todotype = 0
			#2、改变当前主任务对应的所有待办的标志位为 2（被办理）
			UPDATE wdk_wf_todo a SET
				a.status 			= '2'
			WHERE 1=1
				AND a.taskid 		= 临时.taskid
			OPTION 临时.subtaskid <> ""
				a.subtaskid		= 临时.subtaskid
			ENDOPTION
				AND a.todotype		= 临时.todotype
			ENDUPDATE
			
			#3、改变当前id对应的待办任务为1（已完成）
			UPDATE wdk_wf_todo a SET
				a.status 			= '1'
				,a.opertype			=	输入.opertype
			WHERE a.id 				= 输入.id
			ENDUPDATE
			
			UPDATE wdk_wf_todo_his a SET
				a.status 			= '1'
				,a.opertype			=	输入.opertype
			WHERE a.id 				= 输入.id
			ENDUPDATE
		ELSE
			IF 临时.todotype = 3
				#如果阅办（抄办）
				UPDATE wdk_wf_todo a SET
					a.ccstatus 			= '1'
					,a.readdt 		    = 输入.operdt
				WHERE a.id 				= 输入.id
				ENDUPDATE
				
				UPDATE wdk_wf_todo_his a SET
					a.ccstatus 			= '1'
					,a.readdt 		    = 输入.operdt
				WHERE a.id 				= 输入.id
				ENDUPDATE
			ELSE
				#如果是转办，则只关闭自己的待办
				UPDATE wdk_wf_todo a SET
					a.status 			= '1'
					,a.opertype			=	输入.opertype
				WHERE a.id 				= 输入.id
				ENDUPDATE
			ENDIF
		ENDIF
	ENDIF
	
	
	#追回完毕
	IF 输入.flag = 104
		#1、查询当前待办所属的主任务
		SELECT
			taskid		as	taskid
			,subtaskid	as 	subtaskid
		FROM wdk_wf_todo a
		WHERE a.id = 输入.id
		ENDSELECT
		
		临时.taskid 		= taskid
		临时.subtaskid 	= subtaskid
		
		#2、改变当前主任务对应的所有待办的标志位为 3（被追回）
		UPDATE wdk_wf_todo a SET
			a.status 			= '4'
			,a.operdt			= 输入.operdt
		WHERE 1=1
			AND a.taskid 		= 临时.taskid
		OPTION 临时.subtaskid <> ""
			a.subtaskid		= 临时.subtaskid
		ENDOPTION
			AND a.status		= '0'
		ENDUPDATE
		
	
	ENDIF
	
	
	#交办或转办
	IF 输入.flag = 105
		#1、查询当前待办所属的主任务
		SELECT
			taskid		as	taskid
			,subtaskid	as 	subtaskid
		FROM wdk_wf_todo a
		WHERE a.id = 输入.id
		ENDSELECT
		
		临时.taskid 		= taskid
		临时.subtaskid 	= subtaskid
		
		#2、改变当前主任务对应的所有待办的标志位为 2（被办理）
		UPDATE wdk_wf_todo a SET
			a.status 			= '2'
		WHERE 1=1
			AND a.taskid 		= 临时.taskid
		OPTION 临时.subtaskid <> ""
			a.subtaskid		= 临时.subtaskid
		ENDOPTION
		ENDUPDATE
		
		#3、改变当前id对应的待办任务为1（已完成）
		UPDATE wdk_wf_todo a SET
			a.status 			= '1'
			,a.opertype 		= 输入.opertype
		WHERE a.id 				= 输入.id
		ENDUPDATE
	ENDIF
	
	
	
	
	
	
	
	
	
	
	
	
	#201、流程结束，待办记录移动到历史库
	IF 输入.flag = 201
		--1、查找当前实例所有关联的待办记录
		SELECT 
			id							as	ID
			,pdid						as	PDID
			,piid						as	PIID
			,nodename					as	NODENAME
			,nodepage					as	NODEPAGE
			,nodetitle					as	NODETITLE
			,nodetype					as	NODETYPE
			,signtype					as	SIGNTYPE
			,taskid						as	TASKID
			,subtaskid					as	SUBTASKID
			,title						as	TITLE
			,actorid					as	ACTORID
			,actortype					as	ACTORTYPE
			,deptuuid					as	DEPTUUID
			,originactorid				as	ORIGINACTORID
			,originactortype			as	ORIGINACTORTYPE
			,origindeptuuid				as	ORIGINDEPTUUID
			,agenttype					as	AGENTTYPE
			,opertype					as	OPERTYPE
			,opercomment				as	OPERCOMMENT
			,operdt						as	OPERDT
			,overduetime				as	OVERDUETIME
