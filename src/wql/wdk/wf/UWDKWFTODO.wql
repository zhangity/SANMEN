	--任务领取人部门
	输入.lockdeptuuid				TYPEAS	s_id
	
	--任务领取时间
	输入.lockdt						TYPEAS	s_sstring
	
	--待办发送人
	输入.sendactorid					TYPEAS	s_id
	
	--待办发送人类型
